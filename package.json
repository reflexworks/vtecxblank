    "@types/react-dom": "^17.0.1",
    "@types/react-google-recaptcha": "^2.1.0",
    "eslint": "^7.20.0",
    "npm": "^7.6.0",
    "react-google-recaptcha": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/react-bootstrap": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "axios": "^0.21.1",
    "css-loader": "^5.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "url-loader": "^4.1.1",
    "vtecxapi": "^1.9.0",
    "vtecxauth": "1.0.2",
    "vtecxutil": "^1.2.2",
