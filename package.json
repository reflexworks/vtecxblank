    "eslint": "^7.11.0",
    "npm": "^6.14.8",
    "react-bootstrap": "^1.3.0",
    "react-google-recaptcha": "^2.1.0"
  },
  "devDependencies": {
    "@types/react-bootstrap": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "axios": "^0.20.0",
    "css-loader": "^5.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^6.1.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "vtecxapi": "^1.6.9",
    "vtecxauth": "1.0.2",
    "vtecxutil": "^1.1.6",
