{
  "name": "vtecxblank",
  "version": "1.0.0",
  "dependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-google-recaptcha": "^2.1.5",
    "eslint": "^8.31.0",
    "npm": "^9.2.0",
    "react-google-recaptcha": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vtecx/vtecxapi": "^1.9.2",
    "@vtecx/vtecxauth": "^1.0.2",
    "axios": "^1.2.2",
    "css-loader": "^6.7.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "prettier-eslint-cli": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1",
    "vtecxutil": "^1.2.10",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "engines": {
    "node": ">=0.12.5"
  },
  "main": "",
  "scripts": {
    "clean": "rimraf dist",
    "clean:server": "npx vtecxutil clean:server",
    "login": "npx vtecxutil login",
    "upload": "npx vtecxutil upload",
    "upload:data": "npx vtecxutil upload:data",
    "upload:template": "npx vtecxutil upload:template",
    "upload:htmlfolders": "npx vtecxutil upload:htmlfolders",
    "upload:folderacls": "npx vtecxutil upload:folderacls",
    "upload:properties": "npx vtecxutil upload:properties",
    "upload:bigquery.json": "npx vtecxutil upload:bigquery.json",
    "upload:firebase.json": "npx vtecxutil upload:firebase.json",
    "download": "npx vtecxutil download",
    "download:data": "npx vtecxutil download:data",
    "download:template": "npx vtecxutil download:template",
    "download:typings": "npx vtecxutil download:typings",
    "download:folderacls": "npx vtecxutil download:folderacls",
    "download:properties": "npx vtecxutil download:properties",
    "download:bigquery.json": "npx vtecxutil download:bigquery.json",
    "download:firebase.json": "npx vtecxutil download:firebase.json",
    "watch": "npx webpack --mode=production --env externals=true --watch",
    "watch:server": "npx webpack --mode=production --env externals=false --watch",
    "watch:index": "npx webpack --env entry=/components/index.tsx --mode=production --env externals=true --watch",
    "deploy:index": "npx webpack --env entry=/components/index.tsx --mode=production --env externals=true",
    "serve": "npx webpack serve --mode=development --env externals=true",
    "serve:index": "npx webpack serve --open index.html --mode=development --env entry=/components/index.tsx --env externals=true",
    "serve:login": "npx webpack serve --open login.html --mode=development --env entry=/components/login.tsx --env externals=true",
    "test": "jest"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reflexworks/vtecxblank.git"
  }
}